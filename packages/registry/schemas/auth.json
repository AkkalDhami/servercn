{
  "$schema": "https://servercn.vercel.app/schema/schema.registry.json",
  "slug": "auth",
  "runtimes": {
    "node": {
      "frameworks": {
        "express": {
          "databases": {
            "mysql": {
              "orms": {
                "drizzle": {
                  "templates": {
                    "index": {
                      "mvc": "auth/index/mysql/drizzle/mvc",
                      "feature": "auth/index/mysql/drizzle/feature"
                    },
                    "otp": {
                      "mvc": "auth/otp/mysql/drizzle/mvc",
                      "feature": "auth/otp/mysql/drizzle/feature"
                    },
                    "user": {
                      "mvc": "auth/user/mysql/drizzle/mvc",
                      "feature": "auth/user/mysql/drizzle/feature"
                    },
                    "session": {
                      "mvc": "auth/session/mysql/drizzle/mvc",
                      "feature": "auth/session/mysql/drizzle/feature"
                    },
                    "refresh-token": {
                      "mvc": "auth/refresh-token/mysql/drizzle/mvc",
                      "feature": "auth/refresh-token/mysql/drizzle/feature"
                    }
                  },
                  "dependencies": {
                    "runtime": ["mysql2", "drizzle-orm"],
                    "dev": ["drizzle-kit"]
                  }
                }
              }
            },

            "postgresql": {
              "orms": {
                "drizzle": {
                  "templates": {
                    "index": {
                      "mvc": "auth/index/postgresql/drizzle/mvc",
                      "feature": "auth/index/postgresql/drizzle/feature"
                    },
                    "otp": {
                      "mvc": "auth/otp/postgresql/drizzle/mvc",
                      "feature": "auth/otp/postgresql/drizzle/feature"
                    },
                    "user": {
                      "mvc": "auth/user/postgresql/drizzle/mvc",
                      "feature": "auth/user/postgresql/drizzle/feature"
                    },
                    "session": {
                      "mvc": "auth/session/postgresql/drizzle/mvc",
                      "feature": "auth/session/postgresql/drizzle/feature"
                    },
                    "refresh-token": {
                      "mvc": "auth/refresh-token/postgresql/drizzle/mvc",
                      "feature": "auth/refresh-token/postgresql/drizzle/feature"
                    }
                  },
                  "dependencies": {
                    "runtime": ["pg", "drizzle-orm"],
                    "dev": ["@types/pg", "drizzle-kit"]
                  }
                }
              }
            },

            "mongodb": {
              "orms": {
                "mongoose": {
                  "templates": {
                    "index": {
                      "mvc": "auth/index/mongodb/mongoose/mvc",
                      "feature": "auth/index/mongodb/mongoose/feature"
                    },
                    "otp": {
                      "mvc": "auth/otp/mongodb/mongoose/mvc",
                      "feature": "auth/otp/mongodb/mongoose/feature"
                    },
                    "user": {
                      "mvc": "auth/user/mongodb/mongoose/mvc",
                      "feature": "auth/user/mongodb/mongoose/feature"
                    },
                    "session": {
                      "mvc": "auth/session/mongodb/mongoose/mvc",
                      "feature": "auth/session/mongodb/mongoose/feature"
                    },
                    "refresh-token": {
                      "mvc": "auth/refresh-token/mongodb/mongoose/mvc",
                      "feature": "auth/refresh-token/mongodb/mongoose/feature"
                    }
                  },
                  "dependencies": {
                    "runtime": ["mongoose"],
                    "dev": []
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
