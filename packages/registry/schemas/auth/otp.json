{
  "$schema": "http://localhost:3000/schema/schema.registry.json",
  "slug": "auth-otp",
  "runtimes": {
    "node": {
      "frameworks": {
        "express": {
          "databases": {
            "mysql": {
              "orms": {
                "drizzle": {
                  "templates": {
                    "mvc": "auth/otp/mysql/drizzle/mvc",
                    "feature": "auth/otp/mysql/drizzle/feature"
                  }
                }
              }
            },

            "postgresql": {
              "orms": {
                "drizzle": {
                  "templates": {
                    "mvc": "auth/otp/postgresql/drizzle/mvc",
                    "feature": "auth/otp/postgresql/drizzle/feature"
                  }
                }
              }
            },

            "mongodb": {
              "orms": {
                "mongoose": {
                  "templates": {
                    "mvc": "auth/otp/mongodb/mongoose/mvc",
                    "feature": "auth/otp/mongodb/mongoose/feature"
                  }
                }
              }
            }
          }
        }
      }
    }
  },

  "dependencies": {
    "common": {
      "runtime": [],
      "dev": []
    },
    "stack:express": {
      "runtime": ["express"],
      "dev": ["@types/express"]
    },
    "db:mysql": {
      "runtime": ["mysql2"],
      "dev": []
    },

    "db:postgresql": {
      "runtime": ["pg"],
      "dev": ["@types/pg"]
    },

    "orm:mongoose": {
      "runtime": ["mongoose"],
      "dev": []
    },

    "orm:drizzle": {
      "runtime": ["drizzle-orm"],
      "dev": ["drizzle-kit"]
    }
  }
}
