{
  "$schema": "https://servercn.vercel.app/schema/component.registry.json",
  "slug": "oauth",
  "runtimes": {
    "node": {
      "frameworks": {
        "express": {
          "prompt": "Select OAuth provider: ",
          "variants": {
            "google": {
              "label": "Google",
              "templates": {
                "mvc": "oauth/google/mvc",
                "feature": "oauth/google/feature"
              },
              "dependencies": {
                "runtime": [
                  "dotenv",
                  "pino",
                  "pino-pretty",
                  "passport",
                  "passport-google-oauth20"
                ],
                "dev": ["@types/passport-google-oauth20", "@types/passport"]
              },
              "env": [
                "LOG_LEVEL",
                "GOOGLE_CLIENT_ID",
                "GOOGLE_CLIENT_SECRET",
                "GOOGLE_REDIRECT_URI"
              ]
            },
            "github": {
              "label": "GitHub",
              "templates": {
                "mvc": "oauth/github/mvc",
                "feature": "oauth/github/feature"
              },
              "dependencies": {
                "runtime": [
                  "dotenv",
                  "pino",
                  "pino-pretty",
                  "passport",
                  "passport-github2"
                ],
                "dev": ["@types/passport-github2", "@types/passport"]
              },
              "env": [
                "LOG_LEVEL",
                "GITHUB_CLIENT_ID",
                "GITHUB_CLIENT_SECRET",
                "GITHUB_REDIRECT_URI"
              ]
            },
            "google-github": {
              "label": "Google + GitHub",
              "templates": {
                "mvc": "oauth/google-github/mvc",
                "feature": "oauth/google-github/feature"
              },
              "dependencies": {
                "runtime": [
                  "dotenv",
                  "pino",
                  "pino-pretty",
                  "passport",
                  "passport-github2",
                  "passport-google-oauth20"
                ],
                "dev": [
                  "@types/passport-google-oauth20",
                  "@types/passport-github2",
                  "@types/passport"
                ]
              },
              "env": [
                "LOG_LEVEL",
                "GITHUB_CLIENT_ID",
                "GITHUB_CLIENT_SECRET",
                "GITHUB_REDIRECT_URI",
                "GOOGLE_CLIENT_ID",
                "GOOGLE_CLIENT_SECRET",
                "GOOGLE_REDIRECT_URI"
              ]
            }
          }
        }
      }
    }
  }
}
