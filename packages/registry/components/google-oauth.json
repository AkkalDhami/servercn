  "dependencies": {
    "runtime": [
      "express",
      "dotenv",
      "pino",
      "pino-pretty",
      "passport",
      "passport-google-oauth20"
    ],
    "dev": [
      "@types/express",
      "@types/passport-google-oauth20",
      "@types/passport"
    ]
  },
  "env": ["GOOGLE_CLIENT_ID", "GOOGLE_CLIENT_SECRET", "GOOGLE_REDIRECT_URI"]
}
