          "dependencies": {
            "runtime": ["jsonwebtoken"],
            "dev": ["@types/jsonwebtoken"]
          },
          "env": ["JWT_ACCESS_SECRET", "JWT_REFRESH_SECRET"]
        }
      }
    }
  }
}
